{
  "name": "react-module-boilerplate",
  "version": "0.1.0",
  "description": "Sample React presentational components package",
  "keywords": [
    "react",
    "component",
    "boilerplate",
    "ui",
    "system",
    "css-modules",
    "styleguide",
    "snapshot"
  ],
  "author": {
    "name": "Pascal Duez",
    "url": "https://github.com/pascalduez"
  },
  "homepage": "https://github.com/pascalduez/react-module-boilerplate",
  "bugs": "https://github.com/pascalduez/react-module-boilerplate/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/pascalduez/react-module-boilerplate.git"
  },
  "license": "Unlicense",
  "files": [
    "dist",
    "src/**/!(*.spec|*.story).(js|css)",
    "README.md",
    "UNLICENSE"
  ],
  "main": "dist/react-module-boilerplate.cjs.js",
  "module": "dist/react-module-boilerplate.esm.js",
  "browser": "dist/react-module-boilerplate.umd.js",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint src/",
    "lint": "run-s lint:*",
    "typecheck": "flow check src/",
    "typecheck:coverage": "flow-coverage-report -t text -t html -i 'src/**/*.js' -x 'src/**/*.{spec,story}.js'",
    "validate": "run-s lint typecheck",
    "test": "BABEL_ENV=test jest",
    "test:ci": "NODE_ENV=ci run-s validate test",
    "pretest:ci": "rm -rf reports/",
    "styleguide": "start-storybook -p 1337 -c .storybook",
    "playroom": "playroom start",
    "format": "prettier --write ./src/**/*",
    "clean": "rm -r dist/ coverage/ reports/ flow-coverage/",
    "build:flow": "echo \"// @flow\n\nexport * from '../src'\" > dist/react-module-boilerplate.cjs.js.flow",
    "build:css": "cat src/styles/variables.css dist/react-module-boilerplate.css | sponge dist/react-module-boilerplate.css",
    "build": "rollup -c",
    "prebuild": "rm -rf dist/ && mkdir dist/",
    "postbuild": "run-s build:flow build:css",
    "prepare": "run-s build",
    "prepublishOnly": "run-s validate test"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-do-expressions": "^7.6.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.6.3",
    "@babel/runtime-corejs2": "^7.6.3",
    "@storybook/addon-a11y": "^5.2.5",
    "@storybook/addon-docs": "^5.2.5",
    "@storybook/addon-options": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@storybook/theming": "^5.2.5",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "clsx": "^1.0.4",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "flow-bin": "^0.111.1",
    "flow-coverage-report": "^0.6.1",
    "jest": "^24.9.0",
    "jest-html-reporter": "^2.6.2",
    "jest-junit": "^9.0.0",
    "jest-watch-typeahead": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "playroom": "^0.10.1",
    "postcss": "^7.0.21",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-test-renderer": "^16.11.0",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "sponge": "^0.1.0",
    "stylelint": "^11.1.1",
    "stylelint-a11y": "^1.2.2",
    "stylelint-config-css-modules": "^2.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-value-no-unknown-custom-properties": "^2.0.0"
  },
  "peerDependencies": {
    "@babel/runtime-corejs2": "^7.0.0",
    "clsx": "^1.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
