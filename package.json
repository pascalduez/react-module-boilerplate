{
  "name": "react-module-boilerplate",
  "version": "0.1.0",
  "description": "Sample React presentational components package",
  "keywords": [
    "react",
    "component",
    "boilerplate",
    "ui",
    "system",
    "css-modules",
    "styleguide",
    "snapshot"
  ],
  "author": {
    "name": "Pascal Duez",
    "url": "https://github.com/pascalduez"
  },
  "homepage": "https://github.com/pascalduez/react-module-boilerplate",
  "bugs": "https://github.com/pascalduez/react-module-boilerplate/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/pascalduez/react-module-boilerplate.git"
  },
  "license": "Unlicense",
  "files": [
    "dist",
    "index.js",
    "README.md",
    "UNLICENSE"
  ],
  "main": "dist/react-module-boilerplate.cjs.js",
  "module": "dist/react-module-boilerplate.esm.js",
  "browser": "dist/react-module-boilerplate.umd.js",
  "scripts": {
    "lint:css": "stylelint src/**/*.css",
    "lint:js": "eslint src/",
    "lint": "run-s lint:*",
    "typecheck": "flow check src/",
    "typecheck:coverage": "flow-coverage-report -t text -t html -i 'src/**/*.js' -x 'src/**/*.{spec,story}.js'",
    "validate": "run-s lint typecheck",
    "test": "BABEL_ENV=test jest",
    "test:cover": "run-s test --coverage",
    "test:ci": "NODE_ENV=ci run-s validate test",
    "pretest:ci": "rm -rf reports/",
    "styleguide": "start-storybook -p 1337 -c .storybook",
    "clean": "rm -r dist/ coverage/ reports/ flow-coverage/",
    "build:flow": "flow-copy-source -v -i '**/*.{story,spec}.js' src/ dist/cjsm",
    "build": "rollup -c",
    "prebuild": "rm -rf dist/",
    "prepare": "run-s build",
    "prepublishOnly": "run-s validate test:cover"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.48",
    "@babel/core": "^7.0.0-beta.48",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.48",
    "@babel/plugin-proposal-do-expressions": "^7.0.0-beta.48",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.48",
    "@babel/preset-env": "^7.0.0-beta.48",
    "@babel/preset-flow": "^7.0.0-beta.48",
    "@babel/preset-react": "^7.0.0-beta.48",
    "@babel/runtime": "^7.0.0-beta.48",
    "@storybook/addon-a11y": "^3.4.5",
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-info": "^3.4.5",
    "@storybook/addon-knobs": "^3.4.5",
    "@storybook/addon-options": "^3.4.5",
    "@storybook/addons": "^3.4.5",
    "@storybook/react": "^3.4.5",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "classnames": "^2.2.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "esm": "^3.0.37",
    "flow-bin": "^0.73.0",
    "flow-copy-source": "^1.3.0",
    "flow-coverage-report": "^0.5.0",
    "jest": "^23.0.0",
    "jest-enzyme": "^6.0.1",
    "jest-html-reporter": "^2.3.0",
    "jest-junit": "^4.0.0",
    "multistream": "^2.1.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^6.0.22",
    "postcss-loader": "^2.1.5",
    "postcss-preset-env": "^5.1.0",
    "postcss-reporter": "^5.0.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-test-renderer": "^16.4.0",
    "rollup": "^0.59.3",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.1.1",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-size-snapshot": "^0.5.0",
    "string-to-stream": "^1.1.1",
    "stylelint": "^9.2.1",
    "stylelint-config-css-modules": "^1.2.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0-beta.42",
    "classnames": "^2.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
