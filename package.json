{
  "name": "react-module-boilerplate",
  "version": "0.1.0",
  "description": "Sample React presentational components package",
  "keywords": [
    "react",
    "component",
    "boilerplate",
    "ui",
    "system",
    "css-modules",
    "styleguide",
    "snapshot"
  ],
  "author": {
    "name": "Pascal Duez",
    "url": "https://github.com/pascalduez"
  },
  "homepage": "https://github.com/pascalduez/react-module-boilerplate",
  "bugs": "https://github.com/pascalduez/react-module-boilerplate/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/pascalduez/react-module-boilerplate.git"
  },
  "license": "Unlicense",
  "files": [
    "dist",
    "src/**/!(*.spec|*.story).(js|css)",
    "README.md",
    "UNLICENSE"
  ],
  "main": "dist/react-module-boilerplate.cjs.js",
  "module": "dist/react-module-boilerplate.esm.js",
  "browser": "dist/react-module-boilerplate.umd.js",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint src/",
    "lint": "run-s lint:*",
    "typecheck": "flow check src/",
    "typecheck:coverage": "flow-coverage-report -t text -t html -i 'src/**/*.js' -x 'src/**/*.{spec,story}.js'",
    "validate": "run-s lint typecheck",
    "test": "BABEL_ENV=test jest",
    "test:ci": "NODE_ENV=ci run-s validate test",
    "pretest:ci": "rm -rf reports/",
    "styleguide": "start-storybook -p 1337 -c .storybook",
    "playroom": "playroom start",
    "format": "prettier --write ./src/**/*",
    "clean": "rm -r dist/ coverage/ reports/ flow-coverage/",
    "build:flow": "echo \"// @flow\n\nexport * from '../src'\" > dist/react-module-boilerplate.cjs.js.flow",
    "build:css": "cat src/styles/variables.css dist/react-module-boilerplate.css | sponge dist/react-module-boilerplate.css",
    "build": "rollup -c",
    "prebuild": "rm -rf dist/ && mkdir dist/",
    "postbuild": "run-s build:flow build:css",
    "prepare": "run-s build",
    "prepublishOnly": "run-s validate test"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs2": "^7.4.3",
    "@storybook/addon-a11y": "^5.0.10",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-info": "^5.0.10",
    "@storybook/addon-knobs": "^5.0.10",
    "@storybook/addon-options": "^5.0.10",
    "@storybook/addons": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "@storybook/theming": "^5.0.10",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "clsx": "^1.0.4",
    "docz": "^0.13.7",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^0.13.7",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.97.0",
    "flow-coverage-report": "^0.6.1",
    "jest": "^24.7.1",
    "jest-enzyme": "^7.0.2",
    "jest-html-reporter": "^2.5.0",
    "jest-junit": "^6.3.0",
    "jest-watch-typeahead": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "playroom": "^0.9.1",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-reporter": "^6.0.1",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "sponge": "^0.1.0",
    "stylelint": "^10.0.1",
    "stylelint-a11y": "^1.1.11",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-value-no-unknown-custom-properties": "^2.0.0"
  },
  "peerDependencies": {
    "@babel/runtime-corejs2": "^7.0.0",
    "clsx": "^1.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
